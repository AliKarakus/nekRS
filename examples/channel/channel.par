[OCCA]
backend = CUDA
deviceNumber = LOCAL-RANK

[GENERAL] 
polynomialOrder = 7
#startFrom = restart.fld
stopAt = endTime
endTime = 1.0 
dt = 1e-03
timeStepper = bdf2
#writeControl = runTime
#writeInterval = 0.1
writeInterval = 1

[PRESSURE]
preconditioner = semg_amg
residualTol = 1e-09

[VELOCITY]
residualTol = 1e-9
viscosity = 0.05 

[TEMPERATURE]
residualTol = 1e-9
conductivity = 0.001 
